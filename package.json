  },
  "devDependencies": {
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "concurrently": "^3.4.0",
    "jest": "19.0.2",
    "react-native-dotenv": "0.0.3",
    "react-test-renderer": "15.4.2",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-logger": "^2.8.2",
    "remote-redux-devtools": "^0.5.7",
